{
  "name": "malditaes/langfuse-php",
  "description": "A PHP library for interacting with the Langfuse API",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.0",
    "openai-php/client": "^0.10.3",
    "webmozart/assert": "^1.11",
    "ramsey/uuid": "^4.7"
  },
  "autoload": {
    "psr-4": {
      "Langfuse\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Żaba",
      "email": "janekzaba@gmail.com"
    },
    {
      "name": "David Fernández",
      "email": "dfernandez@maldita.es"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
